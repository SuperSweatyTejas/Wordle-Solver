{
    "name": "Wordle-Solver",
    "description": "Extension to auto solve worldes",
    "version": "1.0.0",
    "manifest_version": 2,
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [{
        "matches": [
            "https://www.powerlanguage.co.uk/wordle/*"
        ],
        "js": [
            "./utility/wordlist.js",
            "./util/solver.js"
        ]
    }]
}